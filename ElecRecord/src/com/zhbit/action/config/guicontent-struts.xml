<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<!-- 配置全局结果及异常映射 -->
	<package name="guicontent" namespace="/guicontent" extends="struts-default">
         <action name="guicontent_*" class="com.zhbit.action.guicontent.GuiContentAction" method="{1}">
            <result name="listUI">/WEB-INF/jsp/guicontent/guicontentinfo_listUI.jsp</result>
            <result name="editorUI">/WEB-INF/jsp/guicontent/guicontentinfo_editorUI.jsp</result>
             <result name="addUI">/WEB-INF/jsp/guicontent/guicontentinfo_addUI.jsp</result>
              <result name="detailUI">/WEB-INF/jsp/guicontent/guicontentinfo_detailUI.jsp</result>
            <result name="add" type="redirectAction">
 	         <param name="actionName">guicontent_listUI</param> 
<!-- 	          <param name="guicontent.studentNo">${guicontent.studentNo}</param>  -->
<!--  	          <param name="guicontent.stuName">${guicontent.stuName}</param>  -->
<!-- 	          <param name="guicontent.guidDate">${guicontent.guidDate}</param>  -->
<!--  	          <param name="pageNO">${pageNO}</param>  -->
<!-- 	           <param name="encode">true</param>  -->
	        </result>
            <result name="editor" type="redirectAction">
	          <param name="actionName">guicontent_listUI</param>
	          <param name="guicontent.studentNo">${guicontent.studentNo}</param>
	          <param name="guicontent.stuName">${guicontent.stuName}</param>
	          <param name="guicontent.guidDate">${guicontent.guidDate}</param>
	          <param name="pageNO">${pageNO}</param>
	          <param name="encode">true</param>
	          </result>
	          <result name="delete" type="redirectAction">
	          <param name="actionName">guicontent_listUI</param>
	          <param name="guicontent.studentNo">${guicontent.studentNo}</param>
	          <param name="guicontent.stuName">${guicontent.stuName}</param>
	          <param name="guicontent.guidDate">${guicontent.guidDate}</param>
	          <param name="pageNO">${pageNO}</param>
	          <param name="encode">true</param>
	          </result>
	           <result name="importExcel" type="redirectAction">
	           <param name="actionName">guicontent_listUI</param>
	           </result>
         </action>
        
        
	</package>

</struts>
