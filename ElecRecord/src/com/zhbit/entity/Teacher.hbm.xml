<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.zhbit.entity.Teacher" table="t_teacherinfo" catalog="elecrecord">
        <id name="id" type="java.lang.String">
            <column name="id" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="employNo" type="java.lang.String">
            <column name="employ_no" length="10">
                <comment>教师工号</comment>
            </column>
        </property>
        <property name="employName" type="java.lang.String">
            <column name="employ_name" length="64">
                <comment>教师姓名</comment>
            </column>
        </property>
        <property name="sex" type="java.lang.String">
            <column name="sex" length="2">
                <comment>性别</comment>
            </column>
        </property>
        <property name="birthday" type="java.sql.Timestamp">
            <column name="birthday" length="19">
                <comment>出生日期</comment>
            </column>
        </property>
        <property name="orgId" type="java.lang.String">
            <column name="org_id" length="32">
                <comment>学院id</comment>
            </column>
        </property>
        <property name="orgName" type="java.lang.String">
            <column name="org_name" length="100">
                <comment>学院名称</comment>
            </column>
        </property>
        <property name="department" type="java.lang.String">
            <column name="department" length="100">
                <comment>系（科室）</comment>
            </column>
        </property>
        <property name="telNo" type="java.lang.String">
            <column name="TelNo" length="20">
                <comment>联系电话</comment>
            </column>
        </property>
        <property name="email" type="java.lang.String">
            <column name="email" length="50">
                <comment>E_mail地址</comment>
            </column>
        </property>
        <property name="address" type="java.lang.String">
            <column name="address" length="200">
                <comment>联系地址</comment>
            </column>
        </property>
        <property name="category" type="java.lang.String">
            <column name="category" length="20">
                <comment>教职工类别</comment>
            </column>
        </property>
        <property name="education" type="java.lang.String">
            <column name="education" length="20">
                <comment>学历</comment>
            </column>
        </property>
        <property name="degree" type="java.lang.String">
            <column name="degree" length="20">
                <comment>学位</comment>
            </column>
        </property>
        <property name="duty" type="java.lang.String">
            <column name="duty" length="20">
                <comment>职务</comment>
            </column>
        </property>
        <property name="acdemicTitle" type="java.lang.String">
            <column name="acdemicTitle" length="20">
                <comment>职称</comment>
            </column>
        </property>
        <property name="invigilatorFlag" type="java.lang.String">
            <column name="invigilatorFlag" length="2">
                <comment>可否监考：T：可以；F：不可以</comment>
            </column>
        </property>
        <property name="researchDirection" type="java.lang.String">
            <column name="researchDirection" length="100">
                <comment>研究方向</comment>
            </column>
        </property>
        <property name="summary" type="java.lang.String">
            <column name="summary" length="500">
                <comment>教师简介</comment>
            </column>
        </property>
        <property name="major" type="java.lang.String">
            <column name="major" length="100">
                <comment>专业</comment>
            </column>
        </property>
        <property name="graduate" type="java.lang.String">
            <column name="graduate" length="100">
                <comment>毕业院校</comment>
            </column>
        </property>
        <property name="qualificationFlag" type="java.lang.String">
            <column name="qualificationFlag" length="2">
                <comment>教师资格标志：Y:有；N：无</comment>
            </column>
        </property>
        <property name="jobStatus" type="java.lang.String">
            <column name="jobStatus" length="2">
                <comment>在职状态：Y:在职；N：离职</comment>
            </column>
        </property>
        <property name="level" type="java.lang.String">
            <column name="level" length="20">
                <comment>教师级别</comment>
            </column>
        </property>
        <property name="isLab" type="java.lang.String">
            <column name="isLab" length="2">
                <comment>是否实验室人员：Y：是；N：否</comment>
            </column>
        </property>
        <property name="isOutHire" type="java.lang.String">
            <column name="isOutHire" length="2">
                <comment>是否外聘</comment>
            </column>
        </property>
        <property name="politicalStatus" type="java.lang.String">
            <column name="politicalStatus" length="20">
                <comment>政治面貌</comment>
            </column>
        </property>
        <property name="nation" type="java.lang.String">
            <column name="nation" length="50">
                <comment>民族</comment>
            </column>
        </property>
        <property name="createTime" type="java.sql.Timestamp">
            <column name="create_time" length="19">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" length="32">
                <comment>创建者</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
